<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/rectangleImageA"
        android:layout_width="132dp"
        android:layout_height="151dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="150dp"
        app:srcCompat="@drawable/rectangleone" />

    <ImageView
        android:id="@+id/rectangleImageB"
        android:layout_width="200dp"
        android:layout_height="111dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="25dp"
        android:layout_marginTop="150dp"
        app:srcCompat="@drawable/rectangletwo" />

    <ImageView
        android:id="@+id/rectangleImageC"
        android:layout_width="160dp"
        android:layout_height="102dp"
        android:layout_alignParentStart="true"
        android:layout_below="@id/rectangleImageA"
        android:layout_marginStart="23dp"
        android:layout_marginTop="17dp"
        app:srcCompat="@drawable/rectangleothree" />

    <ImageView
        android:id="@+id/rectangleImageD"
        android:layout_width="153dp"
        android:layout_height="94dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/rectangleImageB"
        android:layout_marginEnd="36dp"
        android:layout_marginTop="60dp"
        app:srcCompat="@drawable/rectanglefour" />

</RelativeLayout>